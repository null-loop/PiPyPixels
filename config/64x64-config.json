{
  "matrix": {
    "overall_led_width": 64,
    "overall_led_height": 64,
    "panel_led_width": 64,
    "panel_led_height": 64,
    "hardware_mapping": "adafruit-hat-pwm",
    "gpio_slowdown": 1,
    "limit_refresh_rate_hz": 0,
    "brightness": 100,
    "disable_hardware_pulsing": false,
    "drop_privileges": false
  },
  "screens": [
    {
      "type": "artwork"
    },
    {
      "type": "bounce"
    },
    {
      "type": "snake"
    },
    {
      "type": "maze",
      "presets": {
        "0": {
          "colours": {
            "EMPTY": [0,0,0],
            "WALL": [180,180,180],
            "SOLVER": [0,255,0],
            "SOLVER_ABANDONED": [0,60,0]
          },
          "frame_rate": 48,
          "scale": 1
        },
        "1": {
          "colours": {
            "EMPTY": [0,0,0],
            "WALL": [180,180,180],
            "SOLVER": [180,0,255],
            "SOLVER_ABANDONED": [90,0,128]
          },
          "frame_rate": 48,
          "scale": 1
        }
      }
    },
    {
      "type": "game-of-life"
    }
  ]
}